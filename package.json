{
  "name": "dm-assist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx server/server.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "backup-locations": "ts-node src/scripts/backupLocations.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "migrate-data": "npx tsx scripts/migrateData.mts",
    "migrate-worlds": "ts-node scripts/migrateWorldsData.mjs",
    "migrate-npcs": "ts-node scripts/migrateNPCsData.mjs",
    "migrate-quests": "ts-node scripts/migrateQuestsData.mjs",
    "migrate-items": "ts-node scripts/migrateItemsData.mjs",
    "migrate-dungeons": "ts-node scripts/migrateDungeons.mjs",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^2.2.3",
    "@heroicons/react": "^2.1.1",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "@tailwindcss/forms": "^0.5.10",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/next": "^8.0.7",
    "@types/three": "^0.158.3",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "delaunator": "^5.0.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "konva": "^8.4.3",
    "next": "^15.3.2",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "react-router-dom": "^7.6.0",
    "three": "^0.158.0",
    "uuid": "^11.1.0"
