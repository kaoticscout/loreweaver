    "@types/delaunator": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.15.21",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.17",
